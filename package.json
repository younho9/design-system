  },
  "license": "MIT",
  "author": "younho9 <younho9.choo@gmail.com>",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
